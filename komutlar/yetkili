const Discord = require('discord.js');
const ayarlar = require('../ayarlar.json');

var prefix = ayarlar.prefix;

exports.run = (client, message, params) => {
  const embedyardim = new Discord.RichEmbed()
  .setTitle("Komutlar")
  .setDescription('')
  .setColor(2067276)
  .addField("** ðŸŽ… Yetkili KomutlarÄ± ðŸŽ…  **", "**h!ban** \n Ä°stediÄŸiniz KiÅŸiyi Sunucudan Banlar. \n**h!kick**  \n Ä°stediÄŸiniz KiÅŸiyi Sunucudan Atar. \n**h!unban** \n Ä°stediÄŸiniz KiÅŸinin YasaÄŸÄ±nÄ± AÃ§ar.\n**h!duyur** \n Sunucudaki herkese Ã¶zelden iletdiÄŸiniz mesajÄ± yazar.\n**h!emojiyazÄ±** \n YazdÄ±ÄŸÄ±nÄ±z mesajÄ± emoji haline getirir.\n**h!yaz** \n Ä°stediÄŸiniz mesajÄ± bota yazdÄ±rÄ±r.\n**h!temizle** \n Belirtilen sayÄ± kadar mesaj siler.\n**h!kilit** \n KanalÄ± istediÄŸiniz kadar sÃ¼reyle kitler.\n**h!uyar** \n BelirlediÄŸiniz kiÅŸiyi uyarÄ±r.Bu kiÅŸiyi #uyarÄ±-alanlar kÄ±smÄ±nda belirir.\n**h!tempmute** \n SeÃ§ilen kiÅŸiyi sÃ¼reli bir ÅŸekilde susturur.\n**h!unmute** \n SeÃ§ilen kiÅŸinin mutesini kaldÄ±rÄ±r.\n**h!rolver** \n Belirlenen kiÅŸiye seÃ§ilen rol verilir.\n**h!efekt** \n Belirtilen resim linkine 'eski' efekti uygular.")
  .setThumbnail(client.user.AvatarURL)
  if (!params[0]) {
    const commandNames = Array.from(client.commands.keys());
    const longest = commandNames.reduce((long, str) => Math.max(long, str.length), 0);
    message.channel.send(embedyardim);
  } else {
    let command = params[0];
    if (client.commands.has(command)) {
      command = client.commands.get(command);
      message.author.send('asciidoc', `= ${command.help.name} = \n${command.help.description}\nDoÄŸru kullanÄ±m: ` + prefix + `${command.help.usage}`);
    }
  }
};

exports.conf = {
  enabled: true,
  guildOnly: false,
  aliases: ['yetkilikomut'],
  permLevel: 0
};

exports.help = {
  name: 'yetkili',
  description: '',
  usage: ':h!yetkili'
};